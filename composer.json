{
  "repositories": [
    {
    "type": "composer",
    "url": "http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.3.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "http://connect.advancedcustomfields.com/index.php?t=5.3.1&p=pro&a=download&k=@@acfProKey"
        }
      }
    }
  ],
  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/regenerate-thumbnails": "*",
    "wpackagist-plugin/wp-migrate-db": "0.7.*",
    "wpackagist-plugin/wp-nested-pages": "1.5.*"
  },
  "extra": {
    "installer-paths": {
      "wwwroot/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wwwroot/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
