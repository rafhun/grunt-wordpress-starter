{
  "repositories": [{
    "type": "composer",
    "url": "http://wpackagist.org"
  }],
  "require": {
    "wpackagist-plugin/advanced-custom-fields": "4.4.*",
    "wpackagist-plugin/wp-migrate-db": "0.7.*",
    "wpackagist-plugin/wp-nested-pages": "1.5.*"
  },
  "extra": {
    "installer-paths": {
      "wwwroot/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wwwroot/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
